<!DOCTYPE html>
<html>
<head>
<title> TEST FORM </title>
<link rel="stylesheet" href="css/styles.css">
</head>
    <body>
      
       
        <div class="table-wrapper" >
            <h1>Hubspot Mini App</h1>
            <table class="fl-table">
        <br>
        <h1><%=userdata.user_id%></h1>
        <h1>Portal Id - <%=userdata.portal_id%></h1>
        <thead>
        <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Mobile number</th>
        
       
        </thead>
        </tr>
        <tbody>
        <%  contacts.forEach(function(contact){ %>
            <tr>
                <td><%=contact.firstname%></td>
                <td><%=contact.lastname%></td>
                <td><%=contact.email%></td>
                <td><%=contact.phone%></td> 
               
                
                </tr>
          <%  }) %>
            </tbody>
            
        </table>
        <form action="/contacts" method="post">
            <button type="submit" name="prev" value="prev">Prev</button>
            <button type ="submit" name="next" value="next"> Next</button>
        </form>
        
       
    </div>
    
    <br>
    
    <button class="button-33" role="button"> <a href="/logout">Log Out</a></button>
    
</body>

</html>