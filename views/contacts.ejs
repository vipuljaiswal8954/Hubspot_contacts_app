<!DOCTYPE html>
<html>
<head>
<title> TEST FORM </title>
<link rel="stylesheet" href="css/styles.css">
</head>
    <body>
      
       
        <div class="table-wrapper" >
            <h1>Hubspot Mini App</h1>
            <table class="fl-table">
        <br>
        <h1><%=userdata.user_id%></h1>
        <h1>Portal Id - <%=userdata.portal_id%></h1>
        <thead>
        <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Mobile number</th>
        
       
        </thead>
        </tr>
        <tbody>
        <%  contacts.forEach(function(contact){ %>
            <tr>
                <td><%=contact.properties.firstname%></td>
                <td><%=contact.properties.lastname%></td>
                <td><%=contact.properties.email%></td>
                <td><%=contact.properties.phone%></td> 
               
                
                </tr>
          <%  }) %>
            </tbody>
            
        </table>
        <% if(status==undefined){ %>
            <h2>No more Contacts</h2>
            <button  onclick="myFunction()" class="button-33" role="button"><<</button>
          <% } else{ %>  
            <button  onclick="myFunction()"  class="button-33" role="button">></button>
         <% } %>
       
    </div>
    
    <br>
    
    <button class="button-33" role="button"> <a href="/vipul/logout">Log Out</a></button>
    
</body>
<script>
    function myFunction() {
      window.location.href = 'https://migration.niswey.net/vipul/contacts';

    }
    </script>
</html>